{
  "functions": {
    "api/sync-supermarkets.js": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/sync-supermarkets",
      "schedule": "0 2 * * *"
    }
  ],
  "env": {
    "GOOGLE_PLACES_API_KEY": "@google_places_api_key",
    "SUPABASE_URL": "@supabase_url", 
    "SUPABASE_SERVICE_KEY": "@supabase_service_key",
    "SYNC_TOKEN": "@sync_token"
  }
}