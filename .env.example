# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Places API Keys
# For web app Google Maps (requires HTTP referrer restrictions)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_web_api_key_here

# For server-side data sync (requires no restrictions or IP restrictions only)
GOOGLE_PLACES_SERVER_API_KEY=your_google_places_server_api_key_here

# Service role key for admin operations and sync scripts
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Admin password for UI access
VITE_ADMIN_PASSWORD=your_admin_password_here